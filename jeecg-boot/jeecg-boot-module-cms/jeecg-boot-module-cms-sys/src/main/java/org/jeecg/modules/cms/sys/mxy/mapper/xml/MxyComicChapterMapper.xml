<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.cms.sys.mxy.mapper.MxyComicChapterMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  mxy_comic_chapter 
		WHERE
			 comic_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.cms.sys.mxy.entity.MxyComicChapter">
		SELECT * 
		FROM  mxy_comic_chapter
		WHERE
			 comic_id = #{mainId} 	</select>
</mapper>
